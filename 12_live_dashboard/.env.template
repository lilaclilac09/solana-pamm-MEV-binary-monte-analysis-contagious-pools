# Solana MEV Dashboard Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control!

# API Keys
HELIUS_API_KEY=your_helius_api_key_here
JITO_API_KEY=your_jito_api_key_here
QUICKNODE_ENDPOINT=your_quicknode_url_here

# RPC Endpoints
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=${HELIUS_API_KEY}
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Dashboard Configuration
DASH_DEBUG=True
DASH_PORT=8050
DASH_HOST=0.0.0.0

# Data Refresh Settings (in seconds)
REFRESH_INTERVAL=60
LIVE_DATA_ENABLED=False

# Caching (Optional - for Redis)
REDIS_URL=redis://localhost:6379

# Security
DASH_SECRET_KEY=your_random_secret_key_here

# Deployment
ENVIRONMENT=development  # Options: development, staging, production
